   "source": [
    "lam = float(input())\n",
    "k = int(input())\n",
    "\n",
    "def fact(k):\n",
    "    kf = 1\n",
    "    for i in range(2,k+1):\n",
    "        kf *= i\n",
    "    return kf\n",
    "\n",
    "e = 2.71828\n",
    "p = ((lam**k)*(e**(-lam)))/fact(k)\n",
    "print(round(p,3))\n"
   ]
  }

    "input()\n",
    "input()\n",
    "\n",
    "def g(n,p):\n",
    "    q = 1-p\n",
    "    x = (q**(n-1))*p\n",
    "    return x\n",
    "\n",
    "p = 1/3\n",
    "gp = 0\n",
    "for i in range(1,6):\n",
    "    gp += g(i,p)\n",
    "\n",
    "print(round(gp,3))\n"
   ]

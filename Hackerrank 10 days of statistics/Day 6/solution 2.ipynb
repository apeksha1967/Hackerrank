   "source": [
    "import math\n",
    "\n",
    "x = int(input())\n",
    "n = int(input())\n",
    "mu = float(input())\n",
    "std = float(input())\n",
    "\n",
    "mu *= n\n",
    "std *= n**0.5\n",
    "\n",
    "def normal(x,mu,std):\n",
    "    N =  0.5 + 0.5 * math.erf((x-mu)/(std* 2**0.5))\n",
    "    return N\n",
    "\n",
    "print(round(normal(x,mu,std),4))\n"
   ]
  }

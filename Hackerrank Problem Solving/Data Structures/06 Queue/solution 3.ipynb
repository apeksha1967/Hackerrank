   "source": [
    "N = 10**6\n",
    "\n",
    "moves = [0] + N * [N]\n",
    "for i in range(N):\n",
    "    moves[i+1] = min(moves[i+1], moves[i] + 1)\n",
    "    j = 2\n",
    "    while j <= i and j * i <= N:\n",
    "        moves[i * j] = min(moves[i * j], moves[i] + 1)\n",
    "        j += 1\n",
    "\n",
    "for _ in range(int(input())):\n",
    "    print(moves[int(input())])"
   ]
  }

    "_,s,k = input(),input(), int(input())\n",
    "def cipher(c):\n",
    "    num = ord(c)\n",
    "    if 97 <= num <= 122:\n",
    "        return chr(((num-97+k)%26)+97)\n",
    "    elif 65 <= num <= 90:\n",
    "        return chr(((num-65+k)%26)+65)\n",
    "    else: return c\n",
    "print(*[cipher(c) for c in s],sep='')\n"
   ]
  }

    "def merge_the_tools(string, k):\n",
    "    # your code goes here\n",
    "    n = len(string)\n",
    "    for x in range(0, n, k):\n",
    "        a = string[x : x+k]\n",
    "        b =[]\n",
    "        for y in a:\n",
    "            if y not in b:\n",
    "                b.append(y)\n",
    "        print (''.join(b))\n",
    "\n"
   ]
  }

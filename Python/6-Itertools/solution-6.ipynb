    "from itertools import combinations\n",
    "k = int(input())\n",
    "m = input().split()\n",
    "\n",
    "d = int(input())\n",
    "n = list(combinations(sorted(m),d))\n",
    "\n",
    "count = 0\n",
    "for b in range(0,len(n)):\n",
    "    if(\"a\" in n[b]):\n",
    "        count += 1\n",
    "print(count/len(n))\n"
   ]
  }
